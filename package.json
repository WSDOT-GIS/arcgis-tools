{
	"name": "@wsdot/arcgis-tools",
	"description": "Tools for working with ArcGIS files",
	"module": "index.ts",
	"type": "module",
	"scripts": {
		"build-exe": "bun build src/dump-stylx.ts --compile --outfile=bin/dump-stylx.exe",
		"build-bun": "bun build --compile --minify --sourcemap --bytecode src/dump-stylx.ts --outfile=bin/dump-stylx",
		"biome": "biome check --write",
		"biome-ci": "biome ci"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"bun-types": "^1.1.31"
	},
	"peerDependencies": {
		"typescript": "^5.6.3"
	},
	"dependencies": {
		"cspell": "^8.15.4"
	},
	"trustedDependencies": ["@biomejs/biome"]
}
